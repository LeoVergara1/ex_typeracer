<script id="list_users_players" type="text/x-handlebars-template">
 <div id="list_users">
  <p class="lead">Lista de jugadores en esta sala: </p>
  <ul class="list-group">
  {{#each userList}}
  <li class="list-group-item justify-content-between imageBackgoround">
    {{this.username}}
    <span class="badge badge-default badge-pill" id="{{{this.username}}}">{{this.score}}</span>
    <div class="scott" id="{{{this.username}}}-sprite" style="margin-left: {{{this.score}}}%">
    </div>
  </li>
  {{/each}}
</ul>
</div>
</script>
